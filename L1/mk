VFD=/home/dara/460/LAB3/mtximage

as86 -o ts.o ts.s
bcc  -c -ansi t.c
ld86 -d -o mtx ts.o t.o OBJ/exec.o mtxlib /usr/lib/bcc/libc.a

echo mount $VFD on /mnt

sudo mount -o loop $VFD /mnt
sudo cp mtx /mnt/boot
sudo umount /mnt

(cd USER; ./mku u1)

echo done

qemu-system-x86_64 -fda mtximage -no-fd-bootchk
